@model Electronic_MVC.Models.Brand_Detail

@{
    ViewData["Title"] = "Details";
}
<!--code of the detail page table-->
<h1>Details</h1>

<div>
    <h4>Brand Detail</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Brand Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Brand_Name)
        </dd>
        <dt class = "col-sm-2">
           Brand Branch Location
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Brand_Branch_Location)
        </dd>
        <dt class = "col-sm-2">
           Brand Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Brand_Email)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
